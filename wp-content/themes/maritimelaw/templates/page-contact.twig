            <hr class="hidden-md visible-sm visible-xs" />
            <div class="col-lg-5 col-md-6">
                <h3><i class="fa fa-phone"></i> Call</h3>
                <p><a href="tel:{{ primary_phone }}">{{ primary_phone }}</a></p>
                <h3><i class="fa fa-telegram"></i> Fax</h3>
                <p>{{ primary_fax }}</p>
                <h3><i class="fa fa-map"></i> Address</h3>
                <p>{{ addr_line_1 }}<br />{{ addr_line_2 }}</p>
                {{ map_embed_code }}
            </div>
        </div>
    </div>
{% endblock %}