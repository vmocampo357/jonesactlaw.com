{% extends 'base-page.twig' %}

{% block page_body %}
    <div id="jal-any-element-primary" class="even-cols col-xs-12 jal-any-element-body content">

        <div class="row">
            <div class="col-xs-12">
                {{ post.content }}
            </div>
        </div>

        <hr />

        <div class="row">
            <div class="col-lg-7 col-md-6">
                <form accept-charset="UTF-8" action="https://tyf.jonesactlaw.com/formCapture.php" data-alt="https://tschedule.infusionsoft.com/app/form/process/3cbbd4623f87fb9add343bc20e44450b" class="infusion-form" id="inf_form_3cbbd4623f87fb9add343bc20e44450b" method="POST">
    <input name="inf_form_xid" type="hidden" value="3cbbd4623f87fb9add343bc20e44450b" />
    <input name="inf_form_name" type="hidden" value="JAL Contact Us Form (NEW)" />
    <input name="infusionsoft_version" type="hidden" value="1.70.0.108055" />
    <div class="infusion-field">
        <label for="inf_field_FirstName">First Name *</label>
        <input class="infusion-field-input" id="inf_field_FirstName" name="inf_field_FirstName" placeholder="First Name *" type="text" />
    </div>
    <div class="infusion-field">
        <label for="inf_field_Email">Email *</label>
        <input class="infusion-field-input" id="inf_field_Email" name="inf_field_Email" placeholder="Email *" type="text" />
    </div>
    <div class="infusion-field">
        <label for="inf_field_Phone1">Phone *</label>
        <input class="infusion-field-input" id="inf_field_Phone1" name="inf_field_Phone1" placeholder="Phone *" type="text" />
    </div>
    <div class="infusion-field">
        <label for="inf_misc_TellUsYourStory">Tell Us Your Story *</label>
        <textarea cols="24" id="inf_misc_TellUsYourStory" name="inf_misc_TellUsYourStory" placeholder="Tell Us Your Story *" rows="5"></textarea></div>
    <div>
        <div>&nbsp;</div>
    </div>
    <div class="infusion-submit">
        <button class="infusion-recaptcha" id="recaptcha_3cbbd4623f87fb9add343bc20e44450b" type="submit">Submit</button>
    </div>
</form>
<script type="text/javascript" src="https://tschedule.infusionsoft.app/app/webTracking/getTrackingCode"></script>
